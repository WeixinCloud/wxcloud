{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "lib",
    "rootDir": "src",
    "strict": true,
    "target": "es2017",
    "noImplicitAny": false,
    "esModuleInterop": true,
    "preserveSymlinks": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "useUnknownInCatchVariables": false
  },
  "include": ["src/**/*"],
  "exclude": ["src/api/cloudapi/crawler"]
}
