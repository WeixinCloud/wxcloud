{
  "name": "@wxcloud/cli",
  "description": "微信云服务 CLI 工具",
  "version": "0.0.3",
  "author": "wxcloud",
  "bin": {
    "wxcloud": "./bin/run"
  },
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "archiver": "^5.3.0",
    "axios": "^0.24.0",
    "change-case": "^4.1.2",
    "cli-ux": "^5.6.3",
    "dotenv": "^10.0.0",
    "inquirer": "^8.2.0",
    "listr": "^0.14.3",
    "node-rsa": "^1.1.1",
    "ora": "^4.0.2",
    "shortid": "^2.2.16",
    "simple-git": "^3.5.0",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@types/chai": "^4",
    "@types/inquirer": "^8.1.3",
    "@types/listr": "^0.14.4",
    "@types/mocha": "^5",
    "@types/node": "^14",
    "@types/shortid": "0.0.29",
    "chai": "^4",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint": "^7.14.0",
    "globby": "^10",
    "mocha": "^5",
    "nyc": "^14",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "keywords": [
    "oclif"
  ],
  "license": "Apache",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "wxcloud",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
